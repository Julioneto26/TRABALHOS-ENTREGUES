#include <stdio.h>

int main()
{
    //CADASTRO DE DADOS DA CARTA 01//
    
    char Codigo[4];
    printf("Digite o código da carta (Exemplo:A01): \n");
    scanf("%3s", cidade); // Limita a entrada a 3 caracteres para evitar overflow

    char cidade[50];
    printf("Digite o nome da cidade da carta A01: \n");
    scanf("%49s", cidade); // Limita a entrada a 49 caracteres para evitar overflow
    
    int populacao;
    printf("Digite o valor de população da carta A01: \n");
    scanf("%d", &populacao);

    float area;
    printf("Digite o valor de Área da carta A01: \n");
    scanf("%f", &area);

    float pib;
    printf("Digite o valor do PIB da carta A01: \n");
    scanf("%f", &pib);

    int pontosTuristicos;
    printf("Digite o número de pontos turísticos da carta A01: \n");
    scanf("%d", &pontosTuristicos);
    
    //CADASTRO DE DADOS DA CARTA 02//
    
    char Codigo2[4];
    printf("Digite o código da carta (Exemplo:B02): \n");
    scanf("%3s", cidade); // Limita a entrada a 3 caracteres para evitar overflow
    
    char cidade2[50];
    printf("Digite o nome da cidade da carta B02: \n");
    scanf("%49s", cidade2); // Limita a entrada a 49 caracteres para evitar overflow
    
    int populacao2;
    printf("Digite o valor de população da carta B02: \n");
    scanf("%d", &populacao2);

    float area2;
    printf("Digite o valor de Área da carta B02: \n");
    scanf("%f", &area2);

    float pib2;
    printf("Digite o valor do PIB da carta B02: \n");
    scanf("%f", &pib2);

    int pontosTuristicos2;
    printf("Digite o número de pontos turísticos da carta B02: \n");
    scanf("%d", &pontosTuristicos2);
    
    printf("Cadastrada cidade A01 Dados: \n");
    printf("Nome %s\n", cidade);
    printf("População %d\n", populacao);
    printf("Área %f\n", area);
    printf("PIB %f\n", pib);
    printf("Pontos turisticos %d\n", pontosTuristicos);

    printf("Cadastrada cidade B02 Dados: \n");
    printf("Nome %s\n", cidade2);
    printf("População %d\n", populacao2);
    printf("Área %f\n", area2);
    printf("PIB %f\n", pib2);
    printf("Pontos turisticos %d\n", pontosTuristicos2);

